        global.opos = 3;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_down)) &amp;&amp; (global.opos == 0) {
        global.opos = 1;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_down)) &amp;&amp; (global.opos == 3) {
        global.opos = 0;
