            <string>/// Keyboard input

if (instance_exists(obj_title_options_controller)) {
    if (keyboard_check_pressed(vk_up)) &amp;&amp; (global.opos &gt; 0) &amp;&amp; (global.opos &lt; 3) {
        global.opos -= 1;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_down)) &amp;&amp; (global.opos &gt; 0) &amp;&amp; (global.opos &lt; 3) {
