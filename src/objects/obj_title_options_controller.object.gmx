        instance_create(0,5,obj_title_arrow_0);
        instance_create(0,5,obj_title_arrow_1);
        instance_create(0,5,obj_title_arrow_2);
        instance_create(0,5,obj_title_arrow_3);
        instance_destroy();
    }
}
