          <argument>
            <kind>1</kind>
            <string>/// Keyboard input

if (instance_exists(obj_title_options)) {
    if (keyboard_check_pressed(vk_up)) &amp;&amp; (global.pos &gt; 0) &amp;&amp; (global.pos &lt; 3) {
        global.pos -= 1;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_down)) &amp;&amp; (global.pos &gt; 0) &amp;&amp; (global.pos &lt; 3) {
        global.pos += 1;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_up)) &amp;&amp; (global.pos == 0) {
        global.pos = 3;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_down)) &amp;&amp; (global.pos == 0) {
        global.pos = 1;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_down)) &amp;&amp; (global.pos == 3) {
        global.pos = 0;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_up)) &amp;&amp; (global.pos == 3) {
        global.pos = 2;
        audio_play_sound(snd_select_choose, 5, false);
    }

    if (keyboard_check_pressed(ord('Z'))) &amp;&amp; (global.pos == 0) {
        audio_play_sound(snd_select_confirm, 5, false);
        //global.door_opened = true;
        room_goto_next();
    } else if (keyboard_check_pressed(ord('Z'))) &amp;&amp; (global.pos == 1) {
        audio_play_sound(snd_select_choose, 5, false);
        instance_create(0,0,obj_howtoplay);
        instance_destroy();
    } else if (keyboard_check_pressed(ord('Z'))) &amp;&amp; (global.pos == 2) {
        audio_play_sound(snd_select_choose, 5, false);
        instance_create(0,5,obj_title_options_controller);
        instance_create(0,5,obj_title_options_base_0);
        instance_create(0,5,obj_title_options_base_1);
        instance_create(0,5,obj_title_options_base_2);
        instance_create(0,5,obj_title_options_base_3);
        instance_create(0,5,obj_options_select_0);
        instance_create(0,5,obj_options_select_1);
        instance_create(0,5,obj_options_select_2);
        instance_create(0,5,obj_options_select_3);
        instance_destroy();
    } else if (keyboard_check_pressed(ord('Z'))) &amp;&amp; (global.pos == 3) {
        audio_play_sound(snd_select_choose, 5, false);
        //instance_create(0,0,obj_leaderboard);
        //instance_create(0,0,obj_highscores);
        //instance_create(0,0,obj_send_in);
        //instance_destroy();
    } else if (keyboard_check_pressed(ord('X'))) {
        audio_play_sound(snd_select_choose, 5, false);
        with (obj_title_options) instance_destroy();
        with (obj_title_arrow_0) instance_destroy();
        with (obj_title_arrow_1) instance_destroy();
        with (obj_title_arrow_2) instance_destroy();
        with (obj_title_arrow_3) instance_destroy();
        instance_create(0,5,obj_title_begin);
    }
    
}
</string>
