            <string>/// Keyboard input

if (instance_exists(obj_menu_start)) {
    if (keyboard_check_pressed(vk_left)) &amp;&amp; (global.pos &gt; 0) &amp;&amp; (global.pos &lt; 2) {
        global.pos -= 1;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_right)) &amp;&amp; (global.pos &gt; 0) &amp;&amp; (global.pos &lt; 2) {
        global.pos += 1;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_left)) &amp;&amp; (global.pos == 0) {
        global.pos = 2;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_right)) &amp;&amp; (global.pos == 0) {
        global.pos = 1;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_right)) &amp;&amp; (global.pos == 2) {
        global.pos = 0;
        audio_play_sound(snd_select_choose, 5, false);
    } else if (keyboard_check_pressed(vk_left)) &amp;&amp; (global.pos == 2) {
        global.pos = 1;
        audio_play_sound(snd_select_choose, 5, false);
    }

    if (keyboard_check_pressed(ord('Z'))) &amp;&amp; (global.pos == 0) {
        audio_play_sound(snd_select_confirm, 5, false);
        //global.door_opened = true;
        room_goto_next();
    } else if (keyboard_check_pressed(ord('Z'))) &amp;&amp; (global.pos == 1) {
        audio_play_sound(snd_select_confirm, 5, false);
    } else if (keyboard_check_pressed(ord('Z'))) &amp;&amp; (global.pos == 2) {
        audio_play_sound(snd_select_confirm, 5, false);
    }
}
