///Transition
if (global.door_opened = true) {
    global.pause = true;
    fade = true;
}

//if (keyboard_check_pressed(vk_anykey)) fade = true;

if (fade) {
    timer += 1/room_speed;
    
    if (timer % 4) {
        if (counter &lt; height) spawn = true;
        counter ++
    }
    
    if (spawn) {
        for (var i = 0; i &lt; width; i++) {
            xx = 16 * i + 8;
            yy = 16 * counter;
            with (instance_create(0, 0, obj_transition)) {
                xx = obj_camera.xx;
                yy = obj_camera.yy;
            }
        }
        
        spawn = false;
    }
    
    if (counter-30 &gt; width) {
        global.transition_black = true;
        counter = -1;
        global.door_opened = false;
        global.pause = false;
        fade = false;
        timer = 1;
    }
}

} //obj_player exist
